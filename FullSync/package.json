  "version": "1.0.0",
  "main": "index.js",
  "private": true,
  "scripts": {
    "start": "node build/index.js",
    "gcp-build": "cp -r /tmp/lib/common-connect /workspace && npm run build && npm pack ../common-connect && npm install common-connect*",
    "start:dev": "concurrently -k \"tsc --watch\" \"nodemon -q build/index.js\"",
    "prebuild": "npm --prefix ../common-connect install && npm --prefix ../common-connect run build",
    "build": "rimraf ./build && tsc",
    "lint": "eslint . --ext .ts",
    "prettier": "prettier --write '**/*.{js,ts}'",
    "test": "npm run prebuild && jest --config jest.config.cjs",
    "test:coverage": "jest --config jest.config.cjs --coverage --collectCoverageFrom='src/**/*.{ts,jxs}'",
    "test:watch": "jest --watch",
    "connector:post-deploy": "node build/connector/post-deploy.js",
    "connector:pre-undeploy": "node build/connector/pre-undeploy.js"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@commercetools-backend/loggers": "^21.19.0",
    "@commercetools/platform-sdk": "^4.1.0",
    "@commercetools/sdk-client-v2": "^2.0.2",
    "@commercetools/ts-client": "^2.1.6",
    "axios": "^1.7.9",
    "common-connect": "file:../common-connect",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "validator": "^13.11.0",
    "xstate": "^5.19.0"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.3",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.2.1",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "concurrently": "^8.2.0",
    "eslint": "^8.46.0",
    "jest": "^29.6.2",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.1",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6"
  },
  "packageManager": "pnpm@9.15.2+sha512.93e57b0126f0df74ce6bff29680394c0ba54ec47246b9cf321f0121d8d9bb03f750a705f24edc3c1180853afd7c2c3b94196d0a3d53d3e069d9e2793ef11f321"
